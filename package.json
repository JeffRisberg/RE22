    "css-loader": "^5.2.7",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "file-loader": "^3.0.0",
    "glob": "^7.2.0",
    "mini-css-extract-plugin": "^0.12.0",
